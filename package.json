{
  "name": "cloudflare-pages-deploy",
  "version": "0.0.11",
  "private": true,
  "description": "A GitHub Action to deploy a CloudFlare Pages Project",
  "main": "dist/index.js",
  "scripts": {
    "build": "cross-env WS_NO_BUFFER_UTIL=1 WS_NO_UTF_8_VALIDATE=1 ncc build src/main.ts --source-map --license licenses.txt",
    "build:pages": "ts-node scripts/build.ts",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "all": "npm run build && npm run format && npm run lint && npm test",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomjschuster/cloudflare-pages-deploy-action.git"
  },
  "keywords": [
    "actions",
    "cloudflare",
    "pages"
  ],
  "author": "Tom Schuster",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.0.3",
    "nanoid": "^3.3.4",
    "node-fetch": "^2.6.7",
    "ts-node": "^10.9.1",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.5",
    "@types/node-fetch": "^2.6.2",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/parser": "^5.33.0",
    "@vercel/ncc": "^0.34.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^26.8.1",
    "jest": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
